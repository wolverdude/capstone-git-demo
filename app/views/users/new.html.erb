<h1>New User!</h1>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>
  <label for="user_username">Username</label>
  <input
    type="text"
    name="user[username]"
    value="<%= @user.username %>"
    id="user_username">

  <label for="user_password">Password</label>
  <input
    type="password"
    name="user[password]"
    id="user_password">

  <label for="user_email">Email</label>
  <input
    type="text"
    name="user[email]"
    id="user_email">

  <input type="submit">
</form>
